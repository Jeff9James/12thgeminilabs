{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "/dist",
  "framework": "vite",
  "installCommand": "npm install --ignore-scripts",
   "env": {
    "VITE_API_URL": "https://web-production-cc201.up.railway.app/api"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
