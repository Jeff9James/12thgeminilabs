{
  "name": "gemini-video-platform-backend",
  "version": "1.0.0",
  "description": "Backend for Gemini 3 Video Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "ts-node src/db/migrate.ts",
    "db:seed": "ts-node src/db/seed.ts",
    "prebuild": "cd ../shared && npm run build"
  },
  "keywords": [
    "video",
    "ai",
    "gemini",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@gemini-video-platform/shared": "file:../shared",
    "@google-cloud/storage": "^7.7.0",
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.7.0",
    "googleapis": "^170.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.17.2",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.16.0",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.8",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
